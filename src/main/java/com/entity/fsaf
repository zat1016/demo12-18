 create table YX_ZT_JJR_BF_FQ
(
  rq VARCHAR2(200) not null,
  sx VARCHAR2(200),
  bz VARCHAR2(200) not null
)
PARTITION BY LIST (bz)
(
 PARTITION part1 VALUES ('节假日') TABLESPACE Part1_tb,
 PARTITION part2 VALUES ('周六日','上班') TABLESPACE Part2_tb
)
tablespace QBSYS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 128K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Add comments to the table
comment on table YX_ZT_JJR_BF_FQ
  is '旧的节假日表备份（11-19年节假日数据）';
-- Add comments to the columns
comment on column YX_ZT_JJR_BF_FQ.rq
  is '日期';
comment on column YX_ZT_JJR_BF_FQ.sx
  is '备注';
comment on column YX_ZT_JJR_BF_FQ.bz
  is '属性';
